{
	"name": "mixer-client-node",
	"version": "2.3.1",
	"description": "A node client for connecting to mixer and the mixer services",
	"main": "lib/index.js",
	"repository": {
		"type": "git",
		"url": "https://github.com/Smart-Programs/mixer-client-node"
	},
	"bugs": {
		"url": "https://github.com/Smart-Programs/mixer-client-node/issues"
	},
	"scripts": {
		"build": "npm i && rimraf lib && tsc --declaration",
		"chat": "tsc && node lib/Tests/ChatTest.js",
		"constellation": "tsc && node lib/Tests/ConstellationTest.js",
		"test": "npm i && npm run chat && npm run constellation"
	},
	"keywords": [ "mixer", "client", "api", "constellation", "library", "oauth", "mixer" ],
	"author": "Garret (itsUnsmart)",
	"license": "MIT",
	"publishConfig": {
		"access": "public"
	},
	"dependencies": {
		"request": "latest",
		"request-promise": "latest",
		"ws": "latest"
	},
	"devDependencies": {
		"@types/node": "latest",
		"@types/request-promise": "latest",
		"rimraf": "latest"
	}
}
