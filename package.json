{
	"name": "mixer-client-node",
	"version": "2.2.4",
	"description": "A node client for connecting to mixer and the mixer services",
	"main": "lib/index.js",
	"repository": {
		"type": "git",
		"url": "https://github.com/Smart-Programs/mixer-client-node"
	},
	"bugs": {
		"url": "https://github.com/Smart-Programs/mixer-client-node/issues"
	},
	"scripts": {
		"build": "rimraf lib && tsc --declaration",
		"prepublish": "npm run build",
		"chat": "npm run build && node lib/Tests/ChatTest.js",
		"constellation": "npm run build && node lib/Tests/ConstellationTest.js",
		"other": "npm run build && node lib/Tests/OtherTest.js",
		"test": "npm run chat && npm run constellation && npm run other"
	},
	"keywords": [
		"mixer",
		"client",
		"api",
		"node",
		"constellation"
	],
	"author": "Garret (itsUnsmart)",
	"license": "ISC",
	"dependencies": {
		"request": "^2.88.0",
		"request-promise": "^4.2.4",
		"ws": "^6.2.1"
	},
	"devDependencies": {
		"@types/node": "^11.13.13",
		"@types/request-promise": "^4.1.44",
		"rimraf": "^2.6.3",
		"tsc": "^1.20150623.0"
	}
}
